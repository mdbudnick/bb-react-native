{
  "expo": {
    "name": "BreathBox",
    "slug": "BreathBox",
    "version": "Release 2.0",
    "orientation": "portrait",
    "icon": "./assets/img/buddha-gnome-smaller.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/buddha-gnome-smaller.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.michaelbudnick.breathbox"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/buddha-gnome.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.michaelbudnick.breathbox"
    },
    "web": {
      "favicon": "./assets/img/buddha-gnome-smaller.png"
    },
    "extra": {
      "eas": {
        "projectId": "a88c3b57-0257-47c5-bc93-75b5285fa606"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ]
  }
}
